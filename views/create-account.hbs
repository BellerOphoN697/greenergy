<script>

  $(document).ready(function () {

    // Function to validate the last name
    function validateLastName() {
        const lastName = $("#last_name").val();
        if (lastName.length === 0) {
            $("#lnameError").text("Name must be at least 1 character");
            return false;
        } else {
            $("#lnameError").text("");
            return true;
        }
    }

    // Function to validate the first name
    function validateFirstName() {
        const firstName = $("#first_name").val();
        if (firstName.length === 0) {
            $("#fnameError").text("Name must be at least 1 character");
            return false;
        } else {
            $("#fnameError").text("");
            return true;
        }
    }

    // Function to validate the email
    function validateEmail() {
        const email = $("#email").val();
        const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
        if (!emailRegex.test(email)) {
            $("#emailError").text("Enter a valid email address");
            return false;
        } else {
            $("#emailError").text("");
            return true;
        }
    }

    // Function to validate the password
    function validatePassword() {
        const password = $("#password").val();
        const confirmPassword = $('#confirmPassword').val();
        if (password !== confirmPassword) {
            $("#passwordError").text("Both passwords should be the same");
            return false;
        } else if (password.length < 6) {
            $("#passwordError").text("Password must be at least 6 characters");
            return false;
        } else {
            $("#passwordError").text("");
            return true;
        }
    }

    // Function to validate the phone number
    function validatePhone() {
        const phone = $("#phone").val();
        if (phone.length !== 10 || isNaN(phone)) {
            $("#phoneError").text("Enter a valid 10-digit phone number");
            return false;
        } else {
            $("#phoneError").text("");
            return true;
        }
    }

    // Function to validate terms acceptance
    function validateTerms() {
        const termsAccepted = $("#terms").prop("checked");
        if (!termsAccepted) {
            $("#termsError").text("Please accept terms and conditions");
            return false;
        } else {
            $("#termsError").text("");
            return true;
        }
    }

    // Initialize termsError text to empty string
    $("#termsError").text("");

    // Function to check form validity and enable/disable proceed button
    function checkFormValidity() {
        const isValid = validateTerms() && validateEmail() && validateLastName() && validateFirstName() && validatePassword() && validatePhone();
        $("#proceed").prop("disabled", !isValid);
    }

    // Call checkFormValidity initially
    checkFormValidity();

    // Event handlers for instant validation
    $("#last_name").on("blur", validateLastName);
    $("#first_name").on("blur", validateFirstName);
    $("#email").on("blur", validateEmail);
    $("#password").on("blur", validatePassword);
    $("#confirmPassword").on("blur", validatePassword);
    $("#phone").on("blur", validatePhone);
    $("#terms").on("change", validateTerms);

    // Form submission handler
    $("#loginform").submit(function (event) {
        // Prevent form submission if validation fails
        if (!validateEmail() || !validateFirstName() || !validateLastName() || !validatePhone() || !validatePassword() || !validateTerms()) {
            event.preventDefault();
        }
    });
});



